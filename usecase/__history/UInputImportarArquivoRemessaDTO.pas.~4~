unit UInputImportarArquivoRemessaDTO;

interface

type
  InputImportarArquivoRemessaDTO = class
  private
    filePath: string;
  public
    constructor create(const filePath: string);
    function getFilePath: string;
  end;

implementation

constructor TInputImportarArquivoRemessaDTO.create(const filePath: string);
begin
  filePath := filePath;
end;

function TInputImportarArquivoRemessaDTO.getFilePath: string;
begin
  Result := filePath;
end;

end.
